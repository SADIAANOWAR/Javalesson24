package day4;

public class ArrayDemo {
	public static void main(String[]args) {
		//declaring an array 
		int myArray[]=new int[5];
		myArray[0]=10;
		myArray[1]=20;
		myArray[2]=30;
		myArray[3]=40;
		myArray[4]=50;
		//extract value from the array
		System.out.println(myArray[4]);//50
	}
	
	
	

}
