package day4;

public class WhileLoop {

	public static void main(String[] args) {
		// loop format-->>initialization;condition/logic;increment/decrements

//		for (int num = 0; num < 10; num++) {
//
//		}

		// print 1 to 5 using while loop
		int num = 1;
		while (num <= 5) {
			System.out.println(num);
			num++;

		}

	}

}
